<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

       <bean id="userDao" class="org.wangy.webtest.dao.impl.UserDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

       <bean id="roleDao" class="org.wangy.webtest.dao.impl.RoleDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

       <bean id="projectDao" class="org.wangy.webtest.dao.impl.ProjectDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

       <bean id="taskDao" class="org.wangy.webtest.dao.impl.TaskDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

       <bean id="aopDao" class="org.wangy.webtest.dao.impl.AopDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

       <bean id="userAuditDao" class="org.wangy.webtest.dao.impl.UserAuditDaoImpl">
              <property name="sessionFactory" ref="sessionFactory"/>
       </bean>

</beans>